{
  "name": "@enhance/starter-project",
  "version": "3.2.0",
  "scripts": {
    "postinstall": "node ./scripts/createManifest.js",
    "start": "sandbox",
    "lint": "eslint ./app/**/*.mjs --fix"
  },
  "devDependencies": {
    "@architect/sandbox": "^5.4.1",
    "@enhance/types": "^0.4.0",
    "eslint": "^8.29.0"
